e'\n      aria-describedby='transition-modal-description'\n      open={open}\n      onClose={handleClose}\n      closeAfterTransition\n      BackdropComponent={Backdrop}\n      BackdropProps={{\n        timeout: 500\n      }}\n      className={styles.modal_container}\n    >\n      <Fade in={open}>\n        <div className={styles.modal}>\n          <div className={styles.row}>\n            <p className={styles.title}>Invite Interviewee/Collaborator</p>\n            <X onClick={handleClose} />\n          </div>\n          <div className={styles.input}>\n            <input\n              type='text'\n              placeholder={'Enter Email'}\n              onChange={onChangeEmail}\n              value={email}\n            />\n            <Send onClick={onSendCollabRequest} />\n          </div>\n          <div className={styles.collaborator_list}>\n            <ul>\n              {interviewee &&\n                interviewee.map((inter,index) => (\n                  <li key={index}>\n                    {inter}\n                    <Trash\n                      onClick={() => {\n                        onDeleteEmail(inter);\n                      }}\n                      style={{ cursor: 'pointer' }}\n                    />\n                  </li>\n                ))}\n            </ul>\n          </div>\n        </div>\n      </Fade>\n    </Modal>\n  );\n};\n\nexport default CollabModal;\n","/mnt/B66886FF6886BE1D/CodePair/client/src/components/home/Menu.js",["270"],"import React from \"react\";\nimport Drawer from \"@material-ui/core/Drawer\";\nimport styles from \"./styles/menu.module.css\";\nimport { Code, UserCheck, Home } from \"react-feather\";\nimport { useHistory } from \"react-router-dom\";\n\nconst Menu = ({ open, handleClose }) => {\n  const history = useHistory();\n  return (\n    <Drawer anchor={\"left\"} open={open} onClose={handleClose}>\n      <div className={styles.menu_container}>\n        <ul>\n          <li\n            onClick={() => {\n           